# Пример кода на Python для поиска наибольшего элемента в списке

def наибольший_элемент(список):
    наибольший = список[0]
    for элемент in список:
        if элемент > наибольший:
            наибольший = элемент
    return наибольший

мой_список = [4, 7, 2, 9, 1, 5, 6]
наибольшее_число = наибольший_элемент(мой_список)
print("Наибольший элемент:", наибольшее_число)
